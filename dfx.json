{
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/main.mo",
      "declarations": {
        "output": "frontend/src/declarations/backend",
        "bindings": ["ts"]
      }
    },
    "frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "dependencies": ["backend"]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
